macos.single.debug = "bin/macos/libwhisper.macos.template_debug.dylib"
macos.double.debug = "bin/macos/libwhisper.macos.template_debug.double.dylib"
macos.single.release = "bin/macos/libwhisper.macos.template_release.dylib"
